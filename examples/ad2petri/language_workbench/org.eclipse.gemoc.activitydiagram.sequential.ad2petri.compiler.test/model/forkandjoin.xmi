<?xml version="1.0" encoding="UTF-8"?>
<activitydiagram:Activity
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:activitydiagram="http://activitydiagram/1.0"
    xsi:schemaLocation="http://activitydiagram/1.0 ../../org.eclipse.gemoc.activitydiagram.sequential.model/ad/activitydiagram.ecore"
    name="forkandjoin">
  <nodes xsi:type="activitydiagram:InitialNode"
      name="init"
      outgoing="init_to_a"/>
  <nodes xsi:type="activitydiagram:OpaqueAction"
      name="A"
      outgoing="a_to_fork"
      incoming="init_to_a"/>
  <nodes xsi:type="activitydiagram:ForkNode"
      name="thefork"
      outgoing="fork_to_b fork_to_d"
      incoming="a_to_fork"/>
  <nodes xsi:type="activitydiagram:OpaqueAction"
      name="B"
      outgoing="b_to_c"
      incoming="fork_to_b"/>
  <nodes xsi:type="activitydiagram:OpaqueAction"
      name="C"
      outgoing="c_to_join"
      incoming="b_to_c"/>
  <nodes xsi:type="activitydiagram:OpaqueAction"
      name="D"
      outgoing="d_to_join"
      incoming="fork_to_d"/>
  <nodes xsi:type="activitydiagram:JoinNode"
      name="thejoin"
      outgoing="join_to_final"
      incoming="c_to_join d_to_join"/>
  <nodes xsi:type="activitydiagram:ActivityFinalNode"
      name="final"
      incoming="join_to_final"/>
  <edges xsi:type="activitydiagram:ControlFlow"
      name="init_to_a"
      source="init"
      target="A"/>
  <edges xsi:type="activitydiagram:ControlFlow"
      name="a_to_fork"
      source="A"
      target="thefork"/>
  <edges xsi:type="activitydiagram:ControlFlow"
      name="fork_to_b"
      source="thefork"
      target="B"/>
  <edges xsi:type="activitydiagram:ControlFlow"
      name="fork_to_d"
      source="thefork"
      target="D"/>
  <edges xsi:type="activitydiagram:ControlFlow"
      name="b_to_c"
      source="B"
      target="C"/>
  <edges xsi:type="activitydiagram:ControlFlow"
      name="c_to_join"
      source="C"
      target="thejoin"/>
  <edges xsi:type="activitydiagram:ControlFlow"
      name="d_to_join"
      source="D"
      target="thejoin"/>
  <edges xsi:type="activitydiagram:ControlFlow"
      name="join_to_final"
      source="thejoin"
      target="final"/>
</activitydiagram:Activity>
